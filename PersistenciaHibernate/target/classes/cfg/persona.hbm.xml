<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.aeat.portalparejas.modelo.entidades.Persona" table="personas">
		<id name="id" column="IDPERSONA" unsaved-value="0">
			<generator class="increment" />
		</id>
		<property name="nombre"  not-null="true"/>
		<property name="edad" not-null="true"/>
		<property name="altura" not-null="true"/>
		<property name="genero" not-null="true"/>
	</class>
</hibernate-mapping>